
<canvas id="daxChart"></canvas>

<script>
    async function getDAXData() {
        try {
            const apiUrl = 'https://www.alphavantage.co/query?function=TIME_SERIES_INTRADAY&symbol=DAX&interval=5min&apikey=AOCXOPNCKC2LZMH7';
            const response = await fetch(apiUrl);
            const data = await response.json();

            const timeSeries = data['Time Series (5min)'];
            const chartLabels = [];
            const chartData = [];

            for (let timestamp in timeSeries) {
                chartLabels.push(timestamp);
                chartData.push(parseFloat(timeSeries[timestamp]['4. close']));
            }

            const ctx = document.getElementById('daxChart').getContext('2d');
            new Chart(ctx, {
                type: 'line',
                data: {
                    labels: chartLabels.reverse(), // Umkehren, um die frühesten Daten zuerst anzuzeigen
                    datasets: [{
                        label: 'DAX Preis',
                        data: chartData.reverse(), // Umkehren für korrekte Reihenfolge
                        borderColor: 'rgba(0, 123, 255, 1)',
                        borderWidth: 2
                    }]
                },
                options: {
                    scales: {
                        y: {
                            beginAtZero: false
                        }
                    }
                }
            });
        } catch (error) {
            console.error('Fehler beim Abrufen der DAX-Daten:', error);
        }
    }

    getDAXData;
</script>