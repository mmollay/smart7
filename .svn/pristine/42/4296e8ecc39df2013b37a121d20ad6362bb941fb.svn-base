function after_form_hedging(id) {

	if (id == 'ok') {
		$('#message').message({ status: 'info', title: 'Das Hedging wurde gespeichert!' });
		$('#modal_form').modal('hide');
		table_reload();
	} else {
		$('#message').message({
			status: 'error', title: 'Fehler im System:' + id
		});
	}

}
